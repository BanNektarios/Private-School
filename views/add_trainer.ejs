<%- include partials/header %>
<%- include partials/navbar %>



<div id="formDiv" class="p3">
    <div class="h1Div">
        <h1 class="col-sm-1-12 h1Form display-4 mr-5"><em> <%= headings %> </em></h1>
    </div>
    <form id="form" action="" method="post">
        <div class="form-group">
            <label for="addTrainer">First Name: </label>
            <input title="Enter only letters!" type="text" class="form-control" name="firstName" id="firstName"
                placeholder="First Name!">
        </div>
        <div class="form-group">
            <label for="addTrainer">Last Name: </label>
            <input title="Enter only letters!" type="text" class="form-control" name="lastName" id="lastName"
                placeholder="Last Name!">
        </div>
        <div class="form-group">
            <label for="addTrainer">Subject: </label>
            <input title="Enter only letters!" type="text" class="form-control" name="subject" id="subject"
                placeholder="Subject!">
        </div>
        <button type="submit" class="btn btn-submit">Submit</button>
    </form>
</div>


<%- include partials/footer %>